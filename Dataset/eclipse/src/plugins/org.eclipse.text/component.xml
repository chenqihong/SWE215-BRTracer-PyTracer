<?xml version="1.0" encoding="UTF-8"?>

<component xmlns="http://eclipse.org/component"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://eclipse.org/component ../component.xsd "
   name="Text">
 <plugin id="org.eclipse.text"/>

 <package name="org.eclipse.jface.text">
   <type name="Assert" instantiate="false"/>
   <type name="ConfigurableLineTracker" subclass="false"/>
   <type name="DefaultLineTracker" subclass="false"/>
   <type name="GapTextStore" subclass="false"/>
   <type name="TextUtilities" subclass="false" instantiate="false"/>
 </package>

 <package name="org.eclipse.jface.text.link">
   <type name="InclusivePositionUpdater" instantiate="false"/>
   <type name="LinkedModeModel" subclass="false"/>
   <type name="LinkedPositionGroup" subclass="false"/>
 </package>

 <package name="org.eclipse.jface.text.projection">
   <type name="ChildDocument" subclass="false" instantiate="false"/>
   <type name="ChildDocumentManager" subclass="false" instantiate="false"/>
   <type name="Fragment" subclass="false" instantiate="false"/>
   <type name="ProjectionDocument" subclass="false" instantiate="false"/>
   <type name="ProjectionDocumentEvent" subclass="false" instantiate="false"/>
   <type name="ProjectionDocumentManager" subclass="false"/>
   <type name="ProjectionMapping" subclass="false" instantiate="false"/>
   <type name="Segment" subclass="false" instantiate="false"/>
 </package>

 <package name="org.eclipse.jface.text.source">
 </package>

 <package name="org.eclipse.jface.text.templates">
   <type name="Template" subclass="false"/>
 </package>

 <package name="org.eclipse.text.edits">
   <type name="TextEdit" subclass="false" instantiate="false"/>
   <type name="TextEditProcessor" subclass="false"/>
   <type name="UndoEdit" subclass="false" instantiate="false"/>
 </package>

 <component-depends unrestricted="true"/>
</component>
