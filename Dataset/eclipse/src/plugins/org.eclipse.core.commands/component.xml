  <type name="ObjectUndoContext"/>
  <type name="OperationHistoryEvent"/>
  <type name="OperationHistoryFactory"/>
  <type name="OperationStatus"/>
  <type name="UndoContext"/>
</package>

<component-depends unrestricted="true"/>

</component>
